{
    "compilerOptions": {
        "outDir": "../dist",
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Node",
        "noEmit": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "rootDir": "./",
        "baseUrl": ".",
        "paths": {
            "#/*": ["./*"]
        }
    },
    "include": ["**/*.ts"]
}
